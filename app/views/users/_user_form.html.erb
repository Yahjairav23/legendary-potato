<%=render partial: "shared/error_messages", locals: {new_object: @user} %>

<%=form_for @user do |t|%> 
    <label>Name:</label>
    <%=t.text_field :name%><br>
    <label>Age:</label>
    <%=t.text_field :age%><br>
    <label>Bio:</label>
    <%=t.text_field :bio%><br>
    <label>Please select the team you are a part of, if any:</label>
    <%=t.collection_select :team_id, Team.all, :id, :name, { include_blank: true}%><br>

    <%=t.submit%>
<%end%>