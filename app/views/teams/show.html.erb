<h1><%=@team.name%></h1>

<h3>Team Members:</h3>
<ul>
<%@team.users.each do |user|%>
    <li><%=link_to user.name, user_path(user)%></li>
<%end%>
</ul>
<br>

<h3>Current Projects:</h3>
<ul>
<%@team.projects.each do |project|%>
    <li><%=link_to project.name, project_path(project)%></li>
<%end%>
</ul>


